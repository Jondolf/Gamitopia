    "rules": {
      "no-console": "off",
      "prettier/prettier": [
        "warn",
        {
          "singleQuote": true
        }
      ]
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
